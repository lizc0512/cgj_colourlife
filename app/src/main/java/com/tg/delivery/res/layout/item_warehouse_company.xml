<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/size_34dp"
    android:id="@+id/rl_warehouse_company"
    android:background="@drawable/bg_warehouse_company">

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_10dp" />

    <TextView
        android:id="@+id/tv_warehouse_company"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/view"
        android:layout_centerHorizontal="true"
        android:text="顺丰快递"
        android:textColor="@color/color_25282e"
        android:textSize="@dimen/font_14sp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_10dp"
        android:layout_below="@id/tv_warehouse_company" />
</RelativeLayout>